<block>
    <view class="page">
        <docs-header title="Tag 标签" __triggerObserer="{{ _triggerObserer }}"></docs-header>
        <view class="doc-body">
            <view class="panel">
                <view class="panel__title">空心标签</view>
                <view class="panel__content">
                    <view class="example-item">
                        <view class="subitem">
                            <at-tag circle="{{true}}" __triggerObserer="{{ _triggerObserer }}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag __triggerObserer="{{ _triggerObserer }}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag circle="{{true}}" __triggerObserer="{{ _triggerObserer }}" active="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag active="{{true}}" __triggerObserer="{{ _triggerObserer }}">标签</at-tag>
                        </view>
                    </view>
                </view>
            </view>
            <view class="panel">
                <view class="panel__title">实心标签</view>
                <view class="panel__content">
                    <view class="example-item">
                        <view class="subitem">
                            <at-tag type="primary" __triggerObserer="{{ _triggerObserer }}" circle="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag type="primary" __triggerObserer="{{ _triggerObserer }}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag type="primary" __triggerObserer="{{ _triggerObserer }}" circle="{{true}}"
                            active="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag type="primary" __triggerObserer="{{ _triggerObserer }}" active="{{true}}">标签</at-tag>
                        </view>
                    </view>
                </view>
            </view>
            <view class="panel">
                <view class="panel__title">点击事件</view>
                <view class="panel__content">
                    <view class="example-item">
                        <view class="subitem" wx:key="{{index}}" wx:for="{{tagList}}" wx:for-item="item"
                        wx:for-index="index">
                            <at-tag name="{{item.name}}" __triggerObserer="{{ _triggerObserer }}"
                            type="primary" active="{{item.active}}" circle="{{index % 2 === 0}}" bindonclick="onClick"
                            __fn_onClick="{{true}}" data-e-onClick-so="this">tag-{{index + 1}}</at-tag>
                        </view>
                    </view>
                </view>
            </view>
            <view class="panel">
                <view class="panel__title">不可点击态</view>
                <view class="panel__content">
                    <view class="example-item">
                        <view class="subitem">
                            <at-tag type="primary" __triggerObserer="{{ _triggerObserer }}" circle="{{true}}"
                            disabled="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag type="primary" __triggerObserer="{{ _triggerObserer }}" disabled="{{true}}">标签</at-tag>
                        </view>
                    </view>
                </view>
            </view>
            <view class="panel">
                <view class="panel__title">空心标签（小）</view>
                <view class="panel__content">
                    <view class="example-item">
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" circle="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" circle="{{true}}"
                            active="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" active="{{true}}">标签</at-tag>
                        </view>
                    </view>
                </view>
            </view>
            <view class="panel">
                <view class="panel__title">实心标签（小）</view>
                <view class="panel__content">
                    <view class="example-item">
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" type="primary"
                            circle="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" type="primary">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" type="primary"
                            circle="{{true}}" active="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" type="primary"
                            active="{{true}}">标签</at-tag>
                        </view>
                    </view>
                </view>
            </view>
            <view class="panel">
                <view class="panel__title">不可点击态（小）</view>
                <view class="panel__content">
                    <view class="example-item">
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" type="primary"
                            circle="{{true}}" disabled="{{true}}">标签</at-tag>
                        </view>
                        <view class="subitem">
                            <at-tag size="small" __triggerObserer="{{ _triggerObserer }}" type="primary"
                            disabled="{{true}}">标签</at-tag>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</block>