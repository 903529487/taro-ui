.at-noticebar {
  position: relative;
  padding: 12rpx 24rpx;
  color: #DE8C17;
  font-size: 0;
  background: #FCF6ED; }
  .at-noticebar__content {
    line-height: 1.5;
    word-break: break-all;
    word-wrap: break-word; }
    .at-noticebar__content-icon {
      display: inline-block;
      margin-right: 12rpx;
      vertical-align: top;
      margin-top: 4rpx; }
    .at-noticebar__content-text {
      display: inline;
      font-size: 24rpx; }
    .at-noticebar__content-inner {
      display: inline; }
  .at-noticebar__close {
    width: 26rpx;
    height: 26rpx;
    position: absolute;
    left: 24rpx;
    top: 50%;
    margin-top: -17rpx; }
    .at-noticebar__close + .at-noticebar__content {
      margin-left: 50rpx; }
  .at-noticebar--marquee .at-noticebar__content-text {
    height: 36rpx;
    line-height: 36rpx;
    overflow: hidden;
    display: block; }
  .at-noticebar--marquee .at-noticebar__content-icon {
    position: absolute; }
    .at-noticebar--marquee .at-noticebar__content-icon + .at-noticebar__content-text {
      margin-left: 45rpx; }
  .at-noticebar--marquee .at-noticebar__content-inner {
    display: inline-block;
    height: 36rpx;
    white-space: nowrap;
    -webkit-animation: marquee 1s linear infinite both;
            animation: marquee 1s linear infinite both;
    -webkit-transform: translateZ(0);
            transform: translateZ(0);
    padding-left: 100%; }
  .at-noticebar--single.at-noticebar--more .at-noticebar__content-text {
    display: inline-block;
    max-width: 525rpx;
    overflow: hidden;
    -o-text-overflow: ellipsis;
       text-overflow: ellipsis;
    white-space: nowrap; }
  .at-noticebar--single.at-noticebar--more .at-noticebar__content-icon + .at-noticebar__content-text {
    max-width: 475rpx; }
  .at-noticebar--more .at-noticebar__content {
    max-width: 525rpx; }
  .at-noticebar--weapp .at-noticebar__content-inner {
    -webkit-animation: none;
            animation: none; }
  .at-noticebar__more {
    position: absolute;
    right: 24rpx;
    top: 12rpx;
    color: #999;
    font-size: 0; }
    .at-noticebar__more .text {
      font-size: 24rpx;
      line-height: 1.5;
      vertical-align: middle; }
    .at-noticebar__more-icon {
      display: inline-block;
      width: 26rpx;
      vertical-align: middle;
      overflow: hidden; }

@-webkit-keyframes marquee {
  100% {
    -webkit-transform: translate3d(-100%, 0, 0);
            transform: translate3d(-100%, 0, 0); } }

@keyframes marquee {
  100% {
    -webkit-transform: translate3d(-100%, 0, 0);
            transform: translate3d(-100%, 0, 0); } }
